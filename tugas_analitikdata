        "\n",
        "spark = SparkSession.builder \\\n",
        "    .appName(\"RDD_Creation_Lab\") \\\n",
        "    .master(\"local[*]\") \\\n",
        "    .getOrCreate()\n",
        "\n",
        "sc = spark.sparkContext\n",
        "\n",
        "print(\"Spark berhasil dijalankan!\")\n"
